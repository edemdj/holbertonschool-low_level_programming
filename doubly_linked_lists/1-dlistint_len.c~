#include "lists_h"
#include <stdio.h>
/**
 * dlistint_len - function that returns the number of elements in
 * a linked dlistint_t list.
 *@h: a dlistint_t list.
 *Return: the number of nodes
 */
size_t dlistint_len(const dlistint_t *h)
{
	size_t nodes = 0;

	while ( h != NULL)
	{
		h = h->next;
		nodes++;

	}

	return (nodes);

}
