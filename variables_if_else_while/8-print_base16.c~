#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
 * main - prints all the numbers of base 16 in lowercase
 *
 * Return: Always 0 (success)
 */
int main(void)
{
        char numbers; char letter;

        for (numbers = '0'; letter <= '9'; numbers++)
		putchar(numbers);

	for (letter = 'a'; letter <= 'f'; letter++)
		putchar(letter)

	putchar ('\n');

	return (0)
}
