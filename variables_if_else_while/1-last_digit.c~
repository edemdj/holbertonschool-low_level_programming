#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
 *main - print the last digit of the number stored in the variable n if it is
 *great than 5,less than 6
 *
 *Return: Always 0 (success)
 */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	last_digit = n % 10;
	if (last_digit > 5)
	{
		printf("Last digit of %d is %d and is greater than 5\n",
		       n, n % 10);
	}
	else if (last_digit < 6)
	{
		printf("Last digit of %d is %d and is less than 6\n",
		       n, n % 10);
	}
	else
	{
		printf("Last digit of %d is %d and is 0\n",
		       n, n % 10);
		
	return (0);
